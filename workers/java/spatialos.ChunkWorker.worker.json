{
  "build": {
    "tasks_filename": "spatialos.java.build.json"
  },
  "bridge": {
    "worker_attribute_set": {
      "attributes": [
        "chunk_worker"
      ]
    },
    "entity_interest": {
      "range_entity_interest": {
        "radius": 2
      }
    },
    "streaming_query": [],
    "component_delivery": {
      "default": "RELIABLE_ORDERED",
      "checkout_all_initially": true
    }
  },
  "external": {
    "default": {
      "run_type": "EXECUTABLE",
      "linux": {
        "command": "java",
        "arguments": [ "-cp", "build/libs/HorizonJavaWorker.jar", "com.hrznstudio.spatial.chunk.ChunkWorker" ]
      },
      "windows": {
        "command": "java",
        "arguments": [ "-cp", "build/libs/HorizonJavaWorker.jar", "com.hrznstudio.spatial.chunk.ChunkWorker" ]
      },
      "macos": {
        "command": "java",
        "arguments": [ "-cp", "build/libs/HorizonJavaWorker.jar", "com.hrznstudio.spatial.chunk.ChunkWorker" ]
      }
    }
  }
}